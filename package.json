    "test-sdk": "ts-node --transpile-only --project tsconfig.scripts.json scripts/test-weavedb-sdk.ts",
    "check-sdk": "ts-node --transpile-only --project tsconfig.scripts.json scripts/check-sdk.ts",
    "check-original-sdk": "ts-node --transpile-only --project tsconfig.scripts.json scripts/check-original-sdk.ts",
    "deploy-all": "npm run deploy-contract && npm run deploy-weavedb-fixed"
