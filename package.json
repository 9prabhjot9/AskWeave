  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run setup && next dev",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 next build",
    "build:skip-types": "NODE_OPTIONS=--max_old_space_size=4096 NEXT_SKIP_TYPE_CHECK=true next build",
    "build:static": "NODE_OPTIONS=--max_old_space_size=4096 next build",
    "export": "next export",
    "build:export": "npm run build && npm run export",
    "build:vercel": "NODE_OPTIONS=--max_old_space_size=4096 next build",
    "build:prod": "./simple-static-export.sh",
    "start": "next start",
    "lint": "next lint",
    "setup": "node scripts/setup.js",
    "deploy-contract": "ts-node --transpile-only --project tsconfig.scripts.json scripts/deploy-contract.ts",
    "deploy-weavedb": "ts-node --transpile-only --project tsconfig.scripts.json scripts/deploy-weavedb.ts",
    "deploy-weavedb-fixed": "ts-node --transpile-only --project tsconfig.scripts.json scripts/deploy-weavedb-fixed.ts",
